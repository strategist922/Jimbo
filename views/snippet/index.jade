                        i.icon-remove
                        | Delete
                    li
                      a(href='JavaScript:void(0);', data-action='editor-select-all') Select All
                    li.divider
                    li
                      a(href='JavaScript:void(0);', data-action='editor-document-refresh')
                        i.icon-refresh
                        | Refresh
                    li.divider
                    li
                      a(href='JavaScript:void(0);', data-action='editor-find')
                        i.icon-search
                        | Find
                    li
                      a(href='JavaScript:void(0);', data-action='editor-find-next') Find Next
                    li
                      a(href='JavaScript:void(0);', data-action='editor-find-previous') Find Previous
                    li
                      a(href='JavaScript:void(0);', data-action='editor-find-replace') Replace...                    
