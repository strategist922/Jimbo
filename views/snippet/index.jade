                  a.dropdown-toggle(data-toggle='dropdown', href='#menuChat')
                    | Chat
                    b.caret
                  ul.dropdown-menu
                    li
                      a(href='JavaScript:void(0);', data-action='editor-videochat') Video Chat
                li#menuHelp.dropdown
                  a.dropdown-toggle(data-toggle='dropdown', href='#menuHelp')
                    | Help
                    b.caret
                  ul.dropdown-menu
                    li
                      a(href='JavaScript:void(0);', data-action='editor-show-about')
                        i.icon-info-sign
                        | About                                    
      .row-fluid        
        #editor-area.span8
          ul.breadcrumb
            li
              a(href='#')            
          textarea#home.tab-pane.fade(disabled='disabled')
        #right-items.span2
