              //- form
              div.idle.modal-header
              div.idle.modal-body
              div.idle.modal-footer
            div
              ul.nav
                li#menuFile.dropdown
                  a.dropdown-toggle(data-toggle='dropdown', href='JavaScript:void(0);')
                    | File
                    b.caret
                  ul.dropdown-menu
                    li                    
                      a(href='JavaScript:void(0);', data-action='editor-new-project')
                        i.icon-list-alt
                        | New Project
                    li
                      a(href='JavaScript:void(0);', data-action='editor-open-project')
                        i.icon-list-alt
                        | Open Project
                    li.divider
                    li
                      a(href='JavaScript:void(0);', data-action='editor-close-project') Close Project                    
                    li.divider
                    li
                      a(href='JavaScript:void(0);', data-action='editor-save-document') Save                                        
                    li.divider
                    li
                      a(href='JavaScript:void(0);', data-action='editor-exit') Exit
                li#menuEdit.dropdown
                  a.dropdown-toggle(data-toggle='dropdown', href='#menuEidt')
                    | Edit
                    b.caret
                  ul.dropdown-menu
                    li
                      a(href='JavaScript:void(0);', data-action='editor-cut-selected-code') Cut
                    li
                      a(href='JavaScript:void(0);', data-action='editor-copy-selected-code') Copy
                    li
                      a(href='JavaScript:void(0);', data-action='editor-paste-code') Paste
